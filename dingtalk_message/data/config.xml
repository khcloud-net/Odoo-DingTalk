<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
		<!--消息类型-->
		<record id="dingtalk_message_type_text" model="dingtalk.message.type">
			<field name="name">文本消息</field>
			<field name="code">text</field>
			<field name="description">文本消息</field>
		</record>

		<record id="dingtalk_message_type_image" model="dingtalk.message.type">
			<field name="name">图片消息</field>
			<field name="code">image</field>
			<field name="description">图片消息</field>
		</record>

		<record id="dingtalk_message_type_voice" model="dingtalk.message.type">
			<field name="name">语音消息</field>
			<field name="code">voice</field>
			<field name="description">语音消息</field>
		</record>

		<record id="dingtalk_message_type_file" model="dingtalk.message.type">
			<field name="name">文件消息</field>
			<field name="code">file</field>
			<field name="description">文件消息</field>
		</record>

		<record id="dingtalk_message_type_url" model="dingtalk.message.type">
			<field name="name">链接消息</field>
			<field name="code">url</field>
			<field name="description">链接消息</field>
		</record>

		<record id="dingtalk_message_type_oa" model="dingtalk.message.type">
			<field name="name">OA消息</field>
			<field name="code">oa</field>
			<field name="description">OA消息</field>
		</record>

		<record id="dingtalk_message_type_markdown" model="dingtalk.message.type">
			<field name="name">markdown消息</field>
			<field name="code">markdown</field>
			<field name="description">markdown消息</field>
		</record>

		<record id="dingtalk_message_type_action_card" model="dingtalk.message.type">
			<field name="name">卡片消息</field>
			<field name="code">card</field>
			<field name="description">卡片消息</field>
		</record>

    </data>
</odoo>